{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/(api|auth)/:path*",
            "destination": "/404"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}